<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
        jcr:primaryType="nt:unstructured"
        jcr:title="SME - Product Breakdown"
        sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <general
                            jcr:primaryType="nt:unstructured"
                            jcr:title="General"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <headline
                                                    jcr:primaryType="nt:unstructured"
                                                    fieldLabel="Headline"
                                                    name="./headline"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </general>
                    <items
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Products"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <viewType
                                                    jcr:primaryType="nt:unstructured"
                                                    path="bt/sme-transform/components/dialog-partials/viewtype"
                                                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                            <visualFieldSet
                                                    jcr:primaryType="nt:unstructured"
                                                    class="benefits-showhide-target"
                                                    showhidetargetvalue="visual"
                                                    sling:resourceType="granite/ui/components/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <visual
                                                            jcr:primaryType="nt:unstructured"
                                                            acs-commons-nested="JSON_STORE"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                            sling:resourceSuperType="bt/sme-transform/components/dialog-partials/halo-product-configuration">
                                                        <field
                                                                jcr:primaryType="nt:unstructured"
                                                                acs-commons-nested="JSON_STORE"
                                                                name="./visual"
                                                                sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <column jcr:primaryType="nt:unstructured">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                    </items>
                                                                </column>
                                                            </items>
                                                        </field>
                                                    </visual>
                                                </items>
                                            </visualFieldSet>
                                            <textOnlyFieldSet
                                                    jcr:primaryType="nt:unstructured"
                                                    class="benefits-showhide-target"
                                                    showhidetargetvalue="textOnly"
                                                    sling:resourceType="granite/ui/components/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <textOnly
                                                            jcr:primaryType="nt:unstructured"
                                                            acs-commons-nested="JSON_STORE"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                            sling:resourceSuperType="bt/sme-transform/components/dialog-partials/halo-product-configuration">
                                                        <field
                                                                jcr:primaryType="nt:unstructured"
                                                                acs-commons-nested="JSON_STORE"
                                                                name="./textOnly"
                                                                sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <column jcr:primaryType="nt:unstructured">
                                                                    <items
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:hideChildren="[image,altText]">
                                                                    </items>
                                                                </column>
                                                            </items>
                                                        </field>
                                                    </textOnly>
                                                </items>
                                            </textOnlyFieldSet>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </items>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
